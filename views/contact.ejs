	<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
	<script type="text/javascript">	
	$(document).ready(function(){
		<%if(login_ck == undefined){%>
			$('.ceos_login_btn').show();
			$('.ceos_signup_btn').show();
		<%} else{%>
			$('.ceos_logout_btn').show();
		<%}%>
	});	
</script>
