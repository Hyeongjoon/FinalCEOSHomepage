	<link rel="stylesheet" type="text/css" href="/stylesheets/MyFontsWebfontsKit.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/index.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/about.css">
	
	<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
	<script type="text/javascript">	
	$(document).ready(function(){
		<%if(login_ck == undefined){%>
			$('.ceos_login_btn').show();
			$('.ceos_signup_btn').show();
		<%} else{%>
			$('.ceos_logout_btn').show();
		<%}%>
	});	
</script>
	<!-- jQuery easing plugin -->
</head>
<body class='home'>
	<div class='container'>
		<div class='top_container'>
			<div class='user_line'>
			</div>
			<div class='logo_line'>
				<div class='logo_box'>
					<img src='/images/ceos_logo.png'/>
				</div>
				<div class='ceos_user_btn_box'>
					<div class='ceos_user_btn ceos_signup_btn'>
					signup
					</div>
					<div class='ceos_user_btn ceos_login_btn'>
					login
					</div>
					<div class='ceos_user_btn ceos_logout_btn'>
					logout
					</div>
				</div>
			</div>
			<div class='menu_line'>
				<div class='menu_box'>
					<div class='menu_name menu_about'>
						ABOUT US
					</div>
					<div class='menu_name menu_board'>
						BOARD
					</div>
					<div class='menu_name menu_mem active'>
						MEMBERS
					</div>
					<div class='menu_name menu_act'>
						ACTIVITIES
					</div>
					<div class='menu_name menu_rec'>
						RECRUIT
					</div>
					<div class='menu_name menu_con'>
						CONTACT
					</div>
				</div>
			</div>
		</div>
		<div class='bottom_container members'>
			<div class='left_col'>
				<div class='left_menu member_menu_all <%if(order==0){%>
				on_page<%}%>'>
					<div class='left_eng' style='width: 30px;'>
					All
					</div>
					<div class='left_kor'>
					전체기수
					</div>
				</div>
				<div class='left_menu member_menu_1 <%if(order==1){%>
				on_page<%}%>'>
					<div class='left_eng' style='width: 85px;'>
					1th 2015-01
					</div>
					<div class='left_kor'>
					1기
					</div>
				</div>
				<div class='left_menu member_menu_2 <%if(order==2){%>
				on_page<%}%>'>
					<div class='left_eng' style='width: 85px;'>
					2th 2015-02
					</div>
					<div class='left_kor'>
					2기
					</div>
				</div>
				<div class='left_menu member_menu_3 <%if(order==3){%>
				on_page<%}%>'>
					<div class='left_eng' style='width: 85px;'>
					3th 2016-01
					</div>
					<div class='left_kor'>
					3기
					</div>
				</div>
				<div class='left_menu member_menu_4 <%if(order==4){%>
				on_page<%}%>'>
					<div class='left_eng' style='width: 85px;'>
					4th 2016-02
					</div>
					<div class='left_kor'>
					4기
					</div>
				</div>
			</div>
			<div class='right_col'>
				<div class='right_top'>
					<div class='right_top_name' style='font-size:40px;'>
					MEMBERS
					</div>
					<div class='right_top_info'>
					미래의 CEO를 꿈꾸는</br> CEOS의 멤버들을 소개합니다.
					</div>
				</div>
				<div class='right_bot'>
							<div class='right_bot_container'>
							<% for( var i=0; i < result.length ; i++){
								for( var j=i+1; j< result.length; j++){
									if(result[i].view_order>result[j].view_order){
										var temp=result[i];
										result[i]=result[j];
										result[j]=temp;
									}
								}
							}
								for( var i = 0 ; i < result.length ; i++){
								if(i % 3 == 0){%><div class='per_card per_first'>
								<div class='per_type inline'>
								<div class='per_type_kor'>
								<%=result[i].CEOSorder%>기
								</div>
								<div class='per_type_eng_long members_type'>
									<%switch(result[i].field){
										case 0 : %>Planner<%break;
										case 1 : %>Developer<%break;
										case 2 : %>Designer<%break;
										}%>
								</div>
							</div>
							<div class='per_img_box'>
								<div class='per_img'>
									<img src="
									<%if(result[i].profile_image!== null){%>
									<%=result[i].profile_image%><%} else {%>
									/images/defalut_face.png
									<%}%>
									"/>
								</div>
								<div class='per_tri_box'>
								</div>
							</div>					
							<div class='per_card_data'>
								<div class='per_card_name_line'>
									<div class='per_card_name_box inline'>
										<div class='per_card_name_kor'>
										<%=result[i].name%>
										</div>
									</div>
									<!--  <div class='per_card_name_eng'>
									Hyorin Lee
									</div>-->		
								</div>
								<div class='per_card_office_line'>
									<div class='per_card_office_name'>
									<%=result[i].univ%> <%=result[i].major%>
									</div>
									<div class='per_card_office_det'>
									CEOS <%=result[i].CEOSorder%>기
									</div>
								</div>
								<div class='per_card_onetalk_line'>
									<%if(result[i].mention !== null){%>
									<%-result[i].mention%><%}%>
								</div> 
								<div class='per_btn_box'>
									<img src='/images/per_face.png'/>
									<img src='/images/per_msg.png'/>
									<img src='/images/per_home.png'/>
								</div>
							</div>
						</div><%}else{%><div class='per_card per_right'>
							<div class='per_type inline'>
								<div class='per_type_kor'>
								<%=result[i].CEOSorder%>기
								</div>
								<div class='per_type_eng_long members_type'>
									<%switch(result[i].field){
										case 0 : %>Planner<%break;
										case 1 : %>Developer<%break;
										case 2 : %>Designer<%break;
										}%>
								</div>
							</div>
							<div class='per_img_box'>
								<div class='per_img'>
									<img src="<%if(result[i].profile_image !== null){%>
									<%=result[i].profile_image%><%} else {%>
									/images/defalut_face.png
									<%}%>"/>
								</div>
								<div class='per_tri_box'>
								</div>
							</div>					
							<div class='per_card_data'>
								<div class='per_card_name_line'>
									<div class='per_card_name_box inline'>
										<div class='per_card_name_kor'>
										<%=result[i].name%>
										</div>
									</div>
									<!--  <div class='per_card_name_eng'>
									Hyorin Lee
									</div>-->		
								</div>
								<div class='per_card_office_line'>
									<div class='per_card_office_name'>
									<%=result[i].univ%> <%=result[i].major%>
									</div>
									<div class='per_card_office_det'>
									CEOS <%=result[i].CEOSorder%>기
									</div>
								</div>
								<div class='per_card_onetalk_line'>
									<%if(result[i].mention !== null){%>
									<%-result[i].mention%><%}%>
								</div>
								<div class='per_btn_box'>
									<img src='/images/per_face.png'/>
									<img src='/images/per_msg.png'/>
									<img src='/images/per_home.png'/>
								</div>
							</div>
						</div><%}}%>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script src="/javascripts/index.js" type="text/javascript"></script>
	<script src="/javascripts/member.js" type="text/javascript"></script>
</body>
</html>